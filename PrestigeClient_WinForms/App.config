<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
<connectionStrings>
  <add name="PrestigeModels"
       connectionString="data source=localhost;initial catalog=Prestige;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
</connectionStrings>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IPrestigeServices" messageEncoding="Mtom" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:12345/" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IPrestigeServices"
                contract="PrestigeServiceReference.IPrestigeServices" name="BasicHttpBinding_IPrestigeServices" />
        </client>
    </system.serviceModel>
</configuration>